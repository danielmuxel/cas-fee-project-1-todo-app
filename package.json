{
  "name": "cas-fee-projekt-1-starter-template",
  "version": "1.0.0",
  "description": "Testat template",
  "main": "public/scripts/main.js",
  "type": "module",
  "scripts": {
    "test": "node test.js",
    "stylelint": "stylelint \"**/*.css\"",
    "w3c": "html-validator --file ./src/app/index.html  --verbose",
    "eslint": "eslint \"**/*.js\"",
    "all": "npm run stylelint && npm run w3c  && npm run eslint  && node -e \"console.log('npm run all completed')\"",
    "api": "node ./src/api/index.js",
    "app": "http-server -p 8080 -o ./src/app/index.html"
  },
  "author": "Michael Gfeller",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.0.1",
    "eslint": "^8.14.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "html-validator-cli": "^2.0.0",
    "prettier": "2.6.2",
    "stylelint": "^14.7.1",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard": "^25.0.0"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "handlebars": "^4.7.7",
    "http-server": "^14.1.1",
    "nedb": "^1.8.0",
    "nodemon": "^2.0.22"
  }
}
